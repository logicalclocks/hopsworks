            <!--        <configuration>-->
            <!--          <failBuildOnCVSS>9</failBuildOnCVSS>-->
            <!--        </configuration>-->
            <executions>
              <execution>
                <goals>
                  <goal>check</goal>
                </goals>
              </execution>
            </executions>
          </plugin>
        </plugins>
      </build>
    </profile>
  </profiles>

